[Unit]
Description=Jenkins Instance
After=docker.service
Requires=docker.service
#After=%p-data@%1.service
#Requires=%p-data@%1.service


[Service]
TimeoutStartSec=0
ExecStartPre=/usr/bin/docker pull jenkins
ExecStartPre=-/usr/bin/docker rm jenkins
ExecStart=/usr/bin/docker run --rm --name jenkins-%i -p 808%i:8080 -p 5000%i:50000 jenkins
ExecStop=/usr/bin/docker stop jenkins
